let ZERO  = \f x -> x
let ONE   = \f x -> f x
let TWO   = \f x -> f (f x)
let THREE = \f x -> f (f (f x))
let FOUR  = \f x -> f (f (f (f x)))

let SUC   = \n f x -> f (n f x)
let ADD   = \n m -> n SUC m
let MUL   = \n m -> n (ADD m) ZERO


-------------------------------------------------------------------------------
-- YOU SHOULD ONLY MODIFY THE TEXT BELOW, JUST THE PARTS MARKED AS COMMENTS
-------------------------------------------------------------------------------
eval suc_one :
  SUC ONE
  =d> (\n f x -> f (n f x)) (\f x -> f x)
  =b> (\f x -> f ((\f x -> f x) f x))
  =b> (\f x -> f ((\x -> f x) x))
  =b> (\f x -> f (f x))
  =d> TWO

eval add_one_zero :
  ADD ONE ZERO
  =d> (\n m -> n SUC m) (\f x -> f x) (\f x -> x)
  =b> (\m -> (\f x -> f x) SUC m) (\f x -> x)
  =b> (\f x -> f x) SUC (\f x -> x)
  =b> (\x -> SUC x) (\f x -> x)
  =b> SUC (\f x -> x)
  =d> (\n f x -> f (n f x)) (\f x -> x)
  =b> (\f x -> f ((\f x -> x) f x)) 
  =b> (\f x -> f ((\x -> x) x)) 
  =b> (\f x -> f x) 
  =d> ONE

eval add_one_two :
